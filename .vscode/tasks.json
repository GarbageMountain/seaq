{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "0.1.0",
  "command": "npm",
  "isShellCommand": true,
  "showOutput": "always",
  "suppressTaskName": true,
  "tasks": [
    {
      "taskName": "install",
      "args": [
        "install"
      ]
    },
    {
      "taskName": "update",
      "args": [
        "update"
      ]
    },
    {
      "taskName": "test",
      "args": [
        "run",
        "test"
      ]
    },
    {
      "taskName": "compile",
      "isBuildCommand": true,
      "args": [
        "run",
        "compile"
      ]
    },
    {
      "taskName": "compile-watch",
      "args": [
        "run",
        "compile-watch"
      ]
    },
    {
      "taskName": "clean-code",
      "args": [
        "run",
        "clean-code"
      ]
    },
    {
      "taskName": "clean-node",
      "args": [
        "run",
        "clean-node"
      ]
    }
  ]
}
