    "@types/faker": "^5.1.6",
    "@types/jest": "^26.0.20",
    "faker": "^5.4.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
